{
  "name": "wicked-animations",
  "version": "1.0.0",
  "description": "A utility-first CSS animation library for slick UI & presentation transitions.",
  "main": "dist/wicked.min.css",
  "style": "dist/wicked.min.css",
  "unpkg": "dist/wicked.min.css",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "sass src/index.scss dist/wicked.css",
    "minify": "cleancss -o dist/wicked.min.css dist/wicked.css",
    "build:all": "npm run build && npm run minify",
    "prepublishOnly": "npm run build:all"
  },
  "author": "Dušan Osmokrović",
  "license": "MIT",
  "keywords": [
    "scss",
    "css",
    "animation",
    "utility",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wicked-animations/library.git"
  },
  "homepage": "https://github.com/wicked-animations/library#",
  "bugs": {
    "url": "https://github.com/wicked-animations/library/issues"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "sass": "^1.86.3"
  }
}
